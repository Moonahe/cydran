{
	"typedocOptions": {
		"name": "Cydran",
		"mode": "file",
		"out": "./docs",
		"module": "umd",
		"target": "es5",
		"includeDeclarations": true,
		"ignoreCompilerErrors": true,
		"hideGenerator": true,
		"exclude": [
			"**/*+(index|.spec|.e2e).ts",
			"**/ObjectUtils.ts",
			"EXCLUSIONS"
		],
		"excludeExternals": true,
		"excludePrivate": true,
		"excludeNotExported": false,
		"categorizeByGroup": true,
		"logger": "console"
	},
	"compilerOptions": {
		"listEmittedFiles": false,
		"module": "commonjs",
		"target": "es5",
		"preserveConstEnums": false,
		"esModuleInterop": true,
		"removeComments": true,
		"sourceMap": true,
		"alwaysStrict": true,
		"charset": "utf-8",
		"declaration": true,
		"declarationDir": "./dist/src/",
		"declarationMap": true,
		"moduleResolution": "Node",
		"allowSyntheticDefaultImports": true,
		"isolatedModules": false,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"noLib": false,
		"noEmitHelpers": false,
		"suppressImplicitAnyIndexErrors": true,
		"outDir": "./dist/src/",
		"newLine": "lf",
		"lib": [
			"es6",
			"DOM"
		],
		"baseUrl": "./src",
		"paths": {
			"@/*": [
				"./*"
			]
		}
	}
}
