{
	"extends": "tslint:recommended",
	"rules": {
		"max-line-length": {
			"options": [
				143
			]
		},
		"new-parens": true,
		"no-arg": true,
		"no-bitwise": true,
		"no-conditional-assignment": true,
		"only-arrow-functions": false,
		"curly": true,
		"no-consecutive-blank-lines": false,
		"indent": [
			true,
			"tabs",
			2
		],
		"max-classes-per-file": false,
		"no-console": {
			"severity": "warning",
			"options": [
				"debug",
				"info",
				"log",
				"time",
				"timeEnd",
				"trace"
			]
		}
	},
	"jsRules": {
		"max-line-length": {
			"options": [
				132
			]
		}
	}
}