{
  "name": "cydran",
  "version": "0.0.28",
  "description": "Unobtrusive Javascript presentation framework",
  "main": "dist/cydran.js",
  "types": "dist/cydran.d.ts",
  "scripts": {
    "start": "npm run clean.all && webpack -w --mode development",
    "build": "npm run clean.all && npm run lint && npm run test && webpack --mode production && rimraf dist/src && npm run zip && npm run zip.map && npm run zip.dts",
    "releaseBuild": "npm run build && npm run minify && npm run zip.mini",
    "clean.all": "rimraf dist",
    "clean.test": "rimraf dist/testBundle.js",
    "test": "webpack --config webpack-test.config.js && npm run clean.test",
    "lint": "tslint --project .",
    "minify": "terser dist/cydran.js --comments /^!/ --ecma 5 --compress --keep-fnames -o dist/cydran.min.js",
    "docs": "rimraf dist/docs && typedoc --mode modules --excludeExternals --excludePrivate --exclude '**/*+(logger|decorator|messaging|error)/**/*.ts' --out dist/docs src",
    "zip": "gzip -9 -c dist/cydran.js > dist/cydran.js.gz",
    "zip.mini": "gzip -9 -c dist/cydran.min.js > dist/cydran.min.js.gz",
    "zip.map": "gzip -9 -c dist/cydran.js.map > dist/cydran.js.map.gz",
    "zip.dts": "gzip -9 -c dist/cydran.d.ts > dist/cydran.d.ts.gz"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cydran/cydran.git"
  },
  "keywords": [
    "MVVM",
    "components",
    "component",
    "framework"
  ],
  "author": "The Cydran team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cydran/cydran/issues"
  },
  "homepage": "https://github.com/cydran/cydran",
  "devDependencies": {
    "@babel/core": "7.7.2",
    "@types/chai": "4.2.4",
    "@types/jsdom": "^12.2.4",
    "@types/lodash": "4.14.146",
    "@types/mocha": "^5.2.7",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.6",
    "babel-preset-env": "1.7.0",
    "chai": "4.2.0",
    "dts-bundle-generator": "3.3.0",
    "fs": "0.0.2",
    "jsdom": "15.2.1",
    "mocha": "6.2.2",
    "raw-loader": "3.1.0",
    "rimraf": "3.0.0",
    "terser": "4.3.10",
    "ts-loader": "6.2.1",
    "tslint": "5.20.1",
    "typedoc": "0.15.0",
    "typescript": "3.7.2",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-node-externals": "1.7.2",
    "webpack-shell-plugin": "0.5.0"
  },
  "dependencies": {}
}
