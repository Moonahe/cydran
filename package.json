{
	"name": "cydran",
	"version": "0.0.24",
	"description": "Unobtrusive Javascript presentation framework",
	"main": "dist/cydran.js",
	"types": "dist/cydran.d.ts",
	"scripts": {
		"start": "node fuse",
		"build": "node fuse build",
		"releaseBuild": "npm run test && npm run lint && node fuse dist",
		"clean": "node fuse clean",
		"test": "node fuse test",
		"lint": "tslint --project .",
		"minify": "uglifyjs dist/cydran.js --keep-fnames -o dist/cydran.min.js",
		"docs": "typedoc --mode modules --excludeExternals --excludePrivate --exclude '**/*+(logger|decorator|messaging)/**/*.ts' --out dist/docs src"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/cydran/cydran.git"
	},
	"keywords": [
		"MVVM",
		"components",
		"component",
		"framework"
	],
	"author": "The Cydran team",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/cydran/cydran/issues"
	},
	"homepage": "https://github.com/cydran/cydran",
	"devDependencies": {
		"@types/chai": "4.1.7",
		"@types/mocha": "^5.2.7",
		"babel-runtime": "6.26.0",
		"chai": "4.2.0",
		"fuse-box": "3.7.1",
		"fuse-test-runner": "1.0.16",
		"mocha": "6.2.2",
		"raw-loader": "3.1.0",
		"terser": "4.3.9",
		"tslint": "5.20.0",
		"typedoc": "0.15.0",
		"typescript": "3.6.4",
		"uglify-js": "3.6.5"
	},
	"dependencies": {}
}
