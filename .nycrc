{
  "check-coverage": true,
  "lines": 50,
  "functions": 50,
  "branches": 50,
  "statements": 50,
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "build/",
    "dist/",
    "src/**/*.spec.ts"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "extension": [
    ".ts"
  ],
  "cache": true,
  "all": true,
  "sourceMap": true,
  "instrument": true
}
